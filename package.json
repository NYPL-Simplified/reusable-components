{
  "name": "library-simplified-reusable-components",
  "version": "1.3.3",
  "description": "Reusable React components for Library Simplified interfaces",
  "repository": {
    "type": "git",
    "url": "https://github.com/NYPL-Simplified/reusable-components.git"
  },
  "main": "dist/reusable-components.js",
  "author": "NYPL",
  "license": "Apache-2.0",
  "scripts": {
    "prepublishOnly": "npm run prod",
    "lint": "tslint -c tslint.json src/*.ts src/*.tsx src/components/*.ts src/components/*.tsx && sass-lint -c .sass-lint.yml -v -q",
    "test": "npm run lint && tsc && cp -r src/stylesheets lib && mocha --require lib/__tests__/testHelper.js lib/__tests__/*.js lib/__tests__/**/*.js",
    "test-file": "npm run lint && tsc && cp -r src/stylesheets lib && mocha --require lib/testHelper.js",
    "dev": "webpack --watch --progress --display-modules --config webpack.dev.js",
    "prod": "webpack --progress --display-modules --config webpack.prod.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@nypl/dgx-svg-icons": "^0.3.9",
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.0.6",
    "@types/node": "^11.13.0",
    "@types/mocha": "^5.2.6",
    "@types/react": "16.4.7",
    "babel-loader": "^8.0.5",
    "bootstrap": "^3.3.6",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^2.3.0",
    "file-loader": "^3.0.1",
    "jsdom": "^9.5.0",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "react-addons-test-utils": "^15.6.2",
    "sass-lint": "^1.9.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.13.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "4.3.1",
    "tslint": "^3.10.2",
    "typescript": "2.7.2",
    "url-loader": "^0.5.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  }
}
